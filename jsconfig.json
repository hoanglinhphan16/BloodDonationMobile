{
  "compilerOptions": {
    "jsx": "react",
    "jsx": "react-jsx",
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@mains": ["src/contents/Main"],
      "@colors": ["src/core/themes/colors"],
      "@fonts": ["src/assets/fonts"],
      "@apis": ["src/core/utils/apis"],
      "@icons": ["src/assets/icons"],
      "@images/*": ["src/assets/images/*"],
      "@core": ["src/core"],
      "@constants/*": ["src/core/constants/*"],
      "@utils/*": ["src/core/utils/*"],
      "@components": ["src/components"],
      "@contents": ["src/contents"]
    }
  },
  "include": ["src", "assets", "react-native.config.js"]
}
